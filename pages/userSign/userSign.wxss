page{
  height:100%;
}
.container{
  width:100%;
  height:100%;
  background: rgb(248,248,248);
  overflow: hidden
}
.card{
    width:90%;
    height:180px;
    background:white;
    margin:40px auto;
    margin-bottom:0;
    box-shadow: 0px 0px 7px  rgb(230,230,230);
}
.image{
    position:relative;
    width:70%;
    height:180px;
    left:0
}
.plusLogo{
    position:relative;
    width:70%;
    height:180px;
    margin:0 auto;
    left:0
}
.uploadId{
    width:90%;
    height:20px;
    font-size:12px;
    text-align: center;
    line-height:20px;
    color:rgb(123,123,123);
    top:120px;
    position: fixed;
}
.o{
    top:310px;
}
.submit{
    width:80%;
    height:60px;
    margin:20px auto;
    margin-bottom:0;
    background: rgb(255,217,62);
    color:black;
    text-align: center;
    line-height:60px;
    font-size:18px;
    letter-spacing: 2px;
    border-radius: 3px;
    margin-top:40px;

}
.infoBox{
  width:90%;
  height:90px;
  /* background:red; */
  margin:10px auto;
  overflow: hidden
}
.input{
  width:96%;
  height:38px;
  outline: none;
  background:none;
  display: block;
  margin:0 auto;
  border:1px solid rgb(194,194,194);
  font-size:14px;
  letter-spacing: 1px;
  text-indent: 3px;
  border-radius: 3px;
}
.timeBox{
  width:90%;
  height:40px;
  margin:10px auto;
}
.timeBox_son{
  width:25%;
  height:40px;
  float:left;
  /* background: yellow; */
  text-align: center;
  line-height: 40px;
  font-size:15px;

}
.timeBox_son_input{
  width:97%;
  height:35px;
  display: block;
  margin-top:2.5px;
  outline: none;
  background:none;
  font-size:14px;
  border:1px solid rgb(194,194,194);
  border-radius: 2px;
}
.radio-group{
  width:100%;
  height:100%;
  display: block;
}
.radio{

}
