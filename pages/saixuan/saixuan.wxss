.btn{
  width:622rpx;
  height:110rpx;
  background: rgb(255,217,62);
  font-size:28rpx;
  font-weight: 800;
  text-align: center;
  line-height:110rpx;
  position: fixed;
  bottom:40rpx;
  margin:0 auto;
  border-radius: 8rpx;
  font-family: PingFang SC;
  letter-spacing: 2rpx;
}
.btnBox{
  width:100%;
  height:200rpx;
  position: fixed;
  bottom:0;
  left:0;
  box-shadow: 0 0 20rpx rgb(230,240,240)
}
.barBtn{
  width:200rpx;
  height:60rpx;
  font-size:28rpx;
