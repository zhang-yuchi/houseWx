/* pages/mime/mime.wxss */
page {
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.headPicBox{
  width:90%;
  height:90px;
  /* background: red; */
  margin:0 auto;
}
.headPicBox_pic{
  width:70px;
  height:70px;
  border-radius: 10px;
  float:left;
  margin-top:10px;
}
.nameBox{
  height:70px;
  margin-top:10px;
  /* background: green; */
  float:right;
}
.name{
  width:100%;
  height:35px;
  font-size: 13px;
  font-weight: 700;
  line-height: 35px;

}
.tel{
  width:100%;
  height:35px;
  font-size: 13px;
  font-weight: 700;
  line-height: 35px;
}
.btnBox{
  width:90%;
  height:50px;
  /* background: red; */
  margin:0 auto;
}
.btnBoxLeft_pic{
  width:30px;
  height:30px;
  margin-top:10px;
  float:left;
}
.btnBoxRight_pic{
  width:24px;
  height:24px;
  float:right;
  margin-top:13px;
}
.text{
  height:50px;
  line-height: 50px;
  margin-left:10px;
  float:left;
  font-size:14px;
  font-weight: 900;
  color:#101010;
}
.other{
  width:90%;
  height:30px;
  color:#A3A3A3;
  font-size: 14px;
  margin:0 auto;
  line-height: 30px;
  
}

/* 联系客服弹窗css代码 */
.concatBox{
  width:100%;
  height:100%;
  background: rgba(187, 187, 187, 0.31);
  position: fixed;
  top:0;
  left:0;
  z-index: 100
}
.concatBox_sonBox{
  width:280px;
  height:146px;
  /* background: red; */
  margin:0 auto;
  background: white;
  border-radius: 4px;
}
.concatBox_sonBox_top{
  width:100%;
  height:106px;
  border-radius: 4px 4px 0 0;
  overflow: hidden;
}
.concatBox_sonBox_bottom{
  width:100%;
  height:40px;
  /* background: green; */
  border-radius: 0 0 4px 4px;
}
.concatBox_sonBox_top_top{
  width:100%;
  height:26px;
  line-height: 26px;
  text-align: center;
  color:#000000;
  font-size:18px;
  margin-top:24px;
}
.concatBox_sonBox_top_bottom{
  width:100%;
  height:26px;
  line-height: 26px;
  text-align: center;
  color:#888888;
  font-size:15px;
}
.concatBox_sonBox_bottom_left{
  width:139px;
  height:39px;
  border-top:1px solid rgba(187, 187, 187, 0.31);
  border-right:1px solid rgba(187, 187, 187, 0.31);
  font-size:18px;
  text-align: center;
  line-height: 39px;
  color:black;
  float:left;
}
.concatBox_sonBox_bottom_right{
  width:140px;
  height:39px;
  border-top:1px solid rgba(187, 187, 187, 0.31);
  font-size:18px;
  text-align: center;
  line-height: 39px;
  color:#02BB00;
  float:left;
}