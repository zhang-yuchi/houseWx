page{
  height:100%;
}
.container{
  width:100%;
  height:100%;
  background: rgb(248,248,248);
  overflow: hidden
}
.card{
    width:70%;
    height:180px;
    background:white;
    margin:40px auto;
    margin-bottom:0;
    box-shadow: 0px 0px 5px  gray;
}
.image{
    position:fixed;
    width:70%;
    height:180px;
}
.plusLogo{
    position:fixed;
    width:70%;
    height:180px;
    margin:0 auto;
}
.uploadId{
    width:90%;
    height:20px;
    font-size:12px;
    text-align: center;
    line-height:20px;
    color:rgb(123,123,123);
    top:120px;
    position: fixed;
}
.o{
    top:310px;
}
.submit{
    width:80%;
    height:60px;
    margin:20px auto;
    margin-bottom:0;
    background: rgb(255,217,62);
    color:black;
    text-align: center;
    line-height:60px;
    font-size:18px;
    letter-spacing: 2px;
    font-family:cursive;
    border-radius: 3px;
    margin-top:40px;

}
