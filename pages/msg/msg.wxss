/* pages/msg/msg.wxss */
page {
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.infoBox{
  width:90%;
  height:100rpx;
  /* background: red; */
  margin:0 auto;
}
.infoBox_pic{
  width:80rpx;
  height:80rpx;
  margin-top:10rpx;
  display: block;
  float:left;
}
.rightBox{
  width:120rpx;
  height:80rpx;
  font-size:24rpx;
  line-height: 80rpx;
  text-align: center;
  color:rgba(0, 0, 0, .54);
  float:right;
  margin-top:10rpx;
}
.middleBox{
  width:200rpx;
  height:100rpx;
  /* background: yellow; */
  float:left;
  margin-left:20rpx;
}
.title{
  height:50rpx;
  font-size:30rpx;
  color:#101010;
  font-weight: 800;

}
.content{
  height:50rpx;
  font-size:30rpx;
  color:#101010;
  font-weight: 800;
}