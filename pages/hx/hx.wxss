/* pages/hx/hx.wxss */
page {
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}


.middle_check {
  width: 95%;
  height: 45px;
  margin: 0 auto;
  border-bottom: 1px solid rgb(245, 245, 245);
}

.middle_check_son {
  height: 45px;
  float: left;
  /* background: red; */
}

.middle_check_son_text {
  height: 45px;
  width: 25px;
  float: left;
  font-size: 12px;
  font-weight: 700;
  line-height: 45px;
  color:rgb(85,85,85);
}

.middle_check_son_pic {
  display: block;
  width: 8px;
  height: 8px;
  margin-top: 18.5px;
  float: left;
}

.middle_check_son_sx {
  width: 15px;
  height: 15px;
  display: block;
  margin: 15px auto;
}
.priceList{
  width:100%;
  height:20px;
  font-size:14px;
  color:#101010;
  text-align: center;
  line-height: 20px;
  font-weight: 800;
  margin-top:30px;
}
.inputBox{
  width:80%;
  height:20px;
  margin:0 auto;
  margin-top:20px;
  /* background: red; */
}
.inputBox_left{
  width:40%;
  height:20px;
  line-height: 20px;
  font-weight: 800;
  font-size:14px;
  color:#101010;
  text-align: left;
  float:left;
}
.inputBox_right{
  width:40%;
  height:20px;
  line-height: 20px;
  font-weight: 800;
  font-size:14px;
  color:#101010;
  text-align: right;
  float:right;
}
.inputBox1{
  width:80%;
  height:30px;
  margin:0 auto;
}
.inputb{
  width:32.5%;
  height:28px;
  display: block;
  float:left;
  border:1px solid #888888;
  font-size: 14px;
  
}
.inputBox1_middle{
  width:33%;
  height:30px;
  line-height: 30px;
  font-weight: 800;
  font-size:14px;
  color:#101010;
  text-align: center;
  float:left;
}
.barBtn{
  width:70px;
  height:30px;
  font-size:14px;
  line-height: 30px;
  text-align: center;
  font-weight: 700;
  float:left;
  color:rgb(86,86,86);
  margin-left:20px;
  border-radius: 4px;
  margin-top:10px;
  background: rgb(247,247,247);
}
.barBtnC{
  width:68px;
  height:28px;
  font-size:14px;
  line-height: 30px;
  text-align: center;
  font-weight: 700;
  float:left;
  color:rgb(86,86,86);
  margin-left:20px;
  border-radius: 4px;
  margin-top:10px;
  background: rgb(247,247,247);
  background:rgb(255,249,224);
  border:1px solid rgb(255,233,144);
}
.btnBox{
  width:100%;
  height:100px;
  position: fixed;
  bottom:0;
  left:0;
  box-shadow: 0 0 10px rgb(230,240,240);

}
.btn{
  width:311px;
  height:55px;
  background: rgb(255,217,62);
  font-size:14px;
  font-weight: 800;
  text-align: center;
  line-height:55px;
  position: fixed;
  bottom:20px;
  margin:0 auto;
  border-radius: 4px;
  font-family: PingFang SC;
  letter-spacing: 1px;
}