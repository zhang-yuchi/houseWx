/* pages/fd_mime/fd_mime.wxss */
page{
  height:100%;
}
.container{
  width:100%;
  height:100%;
  background: rgb(248,248,248);
  overflow: hidden
}
.topBox{
  width:100%;
  height:100px;
}
.topBox_left{
  width:50%;
  height:100%;
  float:left;
}
.topBox_right{
  width:80px;
  height:80px;
  float:left;
  margin-top:10px;
  display: block;
  border-radius: 80px;
  margin-left:50px;
}
.topBox_left_name{
  width:100%;
  height:40px;
  font-size:20px;
  text-align: left;
  line-height: 40px;
  color:#333333;
  margin-top:10px;
  text-indent: 10px;
}
.topBox_left_tel{
  width:100%;
  height:40px;
  font-size:15px;
  text-align: left;
  line-height: 40px;
  color:#333333;
  text-indent: 10px;
}
.title{
  width:100%;
  height:30px;
  font-size:18px;
  text-align: left;
  line-height: 30px;
  color:#333333;
  text-indent: 10px;
  font-weight: 800;
  letter-spacing: 2px;
  margin-top:30px;
}
.cardBox{
  width:100%;
  height:180px;
}
.cardBox_son{
  width:32%;
  height:100%;
  float:left;
  border-radius: 5px;
  
}
.cardBox_son:first-child{
  margin-left:1%;
  margin-right:1%;
}
.cardBox_son:last-child{
  margin-left:1%;
  margin-right:1%;
}
.cardBox_son_pic{
  width:70px;
  height:70px;
  display: block;
  border-radius: 70px;
  margin-top:10px;
  margin-left:10px;
}
.cardBox_son_z{
  width:100%;
  height:20px;
  font-size:13px;
  text-align: left;
  line-height: 30px;
  color:white;
  text-indent: 10px;
  font-weight: 600;
  letter-spacing: 2px;
  margin-top:15px;
}
.cardBox_son_y{
  width:100%;
  height:20px;
  font-size:13px;
  text-align: left;
  line-height: 30px;
  color:white;
  text-indent: 10px;
  font-weight: 600;
  letter-spacing: 2px;
}
.otherBox{
  width:100%;
  height:40px;
  margin-top:15px;
}
.otherBox_pic{
  width:20px;
  height:20px;
  margin-top:10px;
  display:block;
  float:left;
  margin-left:30px;
}
.otherBox_font{
  width:60%;
  height:40px;
  font-size:15px;
  text-align: left;
  line-height: 40px;
  color:#333333;
  font-weight: 600;
  letter-spacing: 2px;
  float:left;
  margin-left:15px;
}
/* 联系客服弹窗css代码 */
.concatBox{
  width:100%;
  height:100%;
  background: rgba(187, 187, 187, 0.31);
  position: fixed;
  top:0;
  left:0;
  z-index: 100
}
.concatBox_sonBox{
  width:280px;
  height:146px;
  /* background: red; */
  margin:0 auto;
  background: white;
  border-radius: 4px;
}
.concatBox_sonBox_top{
  width:100%;
  height:106px;
  border-radius: 4px 4px 0 0;
  overflow: hidden;
}
.concatBox_sonBox_bottom{
  width:100%;
  height:40px;
  /* background: green; */
  border-radius: 0 0 4px 4px;
}
.concatBox_sonBox_top_top{
  width:100%;
  height:26px;
  line-height: 26px;
  text-align: center;
  color:#000000;
  font-size:18px;
  margin-top:24px;
}
.concatBox_sonBox_top_bottom{
  width:100%;
  height:26px;
  line-height: 26px;
  text-align: center;
  color:#888888;
  font-size:15px;
}
.concatBox_sonBox_bottom_left{
  width:139px;
  height:39px;
  border-top:1px solid rgba(187, 187, 187, 0.31);
  border-right:1px solid rgba(187, 187, 187, 0.31);
  font-size:18px;
  text-align: center;
  line-height: 39px;
  color:black;
  float:left;
}
.concatBox_sonBox_bottom_right{
  width:140px;
  height:39px;
  border-top:1px solid rgba(187, 187, 187, 0.31);
  font-size:18px;
  text-align: center;
  line-height: 39px;
  color:#02BB00;
  float:left;
}