  color:rgb(86,86,86);
  margin-left:40rpx;
  border-radius: 8rpx;
  margin-top:20rpx;
  background: rgb(247,247,247);
}
.barBtnC{
  box-sizing: border-box;
  width:200rpx;
  height:60rpx;
  font-size:28rpx;
