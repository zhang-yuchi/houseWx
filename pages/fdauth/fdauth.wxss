/* pages/fdauth/fdauth.wxss */
page {
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.picBox{
  width:92%;
  height:320rpx;
  margin:0 auto;
  border:2rpx solid #BBBBBB;
}
.picBox_icon{
  width:94rpx;
  height:94rpx;
  margin: 0 auto;
  display: block;
  margin-top:80rpx;
}
.picBox_font{
  width:100%;
  height:40rpx;
  font-size:28rpx;
  color:#101010;
  text-align: center;
  line-height: 40rpx;
  margin-top:100rpx;
}
.btn{
  width:386rpx;
  height:94rpx;
  border:2rpx solid #BBBBBB;
  font-size:28rpx;
  font-weight: 800;
  text-align: center;
  line-height:94rpx;
  position: fixed;
  bottom:40rpx;
  margin:0 auto;
  border-radius: 8rpx;
  font-family: PingFang SC;
}