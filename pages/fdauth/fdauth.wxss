/* pages/fdauth/fdauth.wxss */
page {
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.picBox{
  width:92%;
  height:160px;
  margin:0 auto;
  border:1px solid #BBBBBB;
}
.picBox_icon{
  width:47px;
  height:47px;
  margin: 0 auto;
  display: block;
  margin-top:40px;
}
.picBox_font{
  width:100%;
  height:20px;
  font-size:14px;
  color:#101010;
  text-align: center;
  line-height: 20px;
  margin-top:50px;
}
.btn{
  width:193px;
  height:47px;
  border:1px solid #BBBBBB;
  font-size:14px;
  font-weight: 800;
  text-align: center;
  line-height:47px;
  position: fixed;
  bottom:20px;
  margin:0 auto;
  border-radius: 4px;
  font-family: PingFang SC;
}