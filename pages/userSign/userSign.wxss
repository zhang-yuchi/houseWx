page{
  height:100%;
}
.container{
  width:100%;
  height:100%;
  background: rgb(248,248,248);
  overflow: hidden
}
.card{
    width:90%;
    height:360rpx;
    background:white;
    margin:80rpx auto;
    margin-bottom:0;
    box-shadow: 0px 0px 14rpx  rgb(230,230,230);
}
.image{
    position:relative;
    width:70%;
    height:360rpx;
    left:0
}
.plusLogo{
    position:relative;
    width:70%;
    height:360rpx;
    margin:0 auto;
    left:0
}
.uploadId{
    width:90%;
    height:40rpx;
    font-size:24rpx;
    text-align: center;
    line-height:40rpx;
    color:rgb(123,123,123);
    top:240rpx;
    position: fixed;
}
.o{
    top:620rpx;
}
.submit{
    width:80%;
    height:120rpx;
    margin:40rpx auto;
    margin-bottom:0;
    background: rgb(255,217,62);
    color:black;
    text-align: center;
    line-height:120rpx;
    font-size:36rpx;
    letter-spacing: 4rpx;
    border-radius: 6rpx;
    margin-top:80rpx;

}
.infoBox{
  width:90%;
  height:180rpx;
  /* background:red; */
  margin:20rpx auto;
  overflow: hidden
}
.input{
  box-sizing: border-box;
  width:96%;
  height:76rpx;
  outline: none;
  background:none;
  display: block;
  padding: 0 20rpx;
  margin:0 auto;
  border:2rpx solid rgb(194,194,194);
  font-size:28rpx;
  letter-spacing: 2rpx;
  text-indent: 6rpx;
  border-radius: 6rpx;
}
.timeBox{
  width:90%;
  height:80rpx;
  margin:20rpx auto;
}
.timeBox_son{
  width:25%;
  height:80rpx;
  float:left;
  /* background: yellow; */
  text-align: center;
  line-height: 80rpx;
  font-size:30rpx;

}
.timeBox_son_input{
  width:97%;
  height:70rpx;
  display: block;
  margin-top:5rpx;
  outline: none;
  background:none;
  font-size:28rpx;
  border:2rpx solid rgb(194,194,194);
  border-radius: 4rpx;
}
.radio-group{
  width:100%;
  height:100%;
  display: block;
}
.radio{

}
