/* pages/fd_mime/fd_mime.wxss */
page{
  height:100%;
}
.container{
  background-color: white;
  width:100%;
  overflow: hidden
}
.topBox{
  display: flex;
  width:100%;
  height:100px;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.topBox_left{
  width:50%;
  height:100%;
  float:left;
}
.topBox_right{
  width:160rpx;
  height:160rpx;
  float:left;
  margin-top:20rpx;
  display: block;
  border-radius: 160rpx;
  margin-left:100rpx;
}
.topBox_left_name{
  width:100%;
  height:80rpx;
  font-size:40rpx;
  text-align: left;
  line-height: 80rpx;
  color:#333333;
  margin-top:20rpx;
  /* text-indent: 20rpx; */
}
.topBox_left_tel{
  width:100%;
  height:80rpx;
  font-size:30rpx;
  text-align: left;
  line-height: 80rpx;
  color:rgb(190, 190, 190);
  /* text-indent: 20rpx; */
}
.title{
  width:100%;
  height:60rpx;
  font-size:36rpx;
  text-align: left;
  line-height: 60rpx;
  color:#333333;
  /* text-indent: 20rpx; */
  font-weight: 800;
  letter-spacing: 4rpx;
  margin-top:60rpx;
  padding: 0 30rpx;
}
.cardBox{
  padding-left: 15rpx;
  display: flex;
  justify-content: space-between;
  width:100%;
  height:360rpx;
}
.cardBox_son{
  width:32%;
  height:100%;
  float:left;
  border-radius: 10rpx;
  
}
.cardBox_son:first-child{
  /* margin-left:1%;
  margin-right:1%; */
}
.cardBox_son:last-child{
  /* margin-left:1%;
  margin-right:1%; */
}
.cardBox_son_pic{
  width:140rpx;
  height:140rpx;
  display: block;
  border-radius: 140rpx;
  margin-top:20rpx;
  margin-left:20rpx;
}
.cardBox_son_z{
  width:100%;
  height:40rpx;
  font-size:26rpx;
  text-align: left;
  line-height: 60rpx;
  color:white;
  text-indent: 20rpx;
  font-weight: 600;
  letter-spacing: 4rpx;
  margin-top:30rpx;
}
.cardBox_son_y{
  width:100%;
  height:40rpx;
  font-size:26rpx;
  text-align: left;
  line-height: 60rpx;
  color:white;
  text-indent: 20rpx;
  font-weight: 600;
  letter-spacing: 4rpx;
}
.otherBox{
  padding-left: 30rpx;
  padding-right: 30rpx;
  width:100%;
  height:80rpx;
  margin-top:30rpx;
}
.otherBox_pic{
  width:40rpx;
  height:40rpx;
  margin-top:20rpx;
  display:block;
  float:left;
  /* margin-left:60rpx; */
}
.otherBox_font{
  margin-right: 100rpx;
  width:60%;
  height:80rpx;
  font-size:30rpx;
  text-align: left;
  line-height: 80rpx;
  color:#333333;
  font-weight: 600;
  letter-spacing: 4rpx;
  float:left;
  margin-left:30rpx;
}
/* 联系客服弹窗css代码 */
.concatBox{
  width:100%;
  height:100%;
  background: rgba(187, 187, 187, 0.31);
  position: fixed;
  top:0;
  left:0;
  z-index: 100
}
.concatBox_sonBox{
  width:560rpx;
  height:292rpx;
  margin:0 auto;
  background: white;
  border-radius: 8rpx;
}
.concatBox_sonBox_top{
  width:100%;
  height:212rpx;
  border-radius: 8rpx 8rpx 0 0;
  overflow: hidden;
}
.concatBox_sonBox_bottom{
  width:100%;
  height:80rpx;
  border-radius: 0 0 8rpx 8rpx;
}
.concatBox_sonBox_top_top{
  width:100%;
  height:52rpx;
  line-height: 52rpx;
  text-align: center;
  color:#000000;
  font-size:36rpx;
  margin-top:48rpx;
}
.concatBox_sonBox_top_bottom{
  width:100%;
  height:52rpx;
  line-height: 52rpx;
  text-align: center;
  color:#888888;
  font-size:30rpx;
}
.concatBox_sonBox_bottom_left{
  width:278rpx;
  height:78rpx;
  border-top:2rpx solid rgba(187, 187, 187, 0.31);
  border-right:2rpx solid rgba(187, 187, 187, 0.31);
  font-size:36rpx;
  text-align: center;
  line-height: 78rpx;
  color:black;
  float:left;
}
.concatBox_sonBox_bottom_right{
  width:280rpx;
  height:78rpx;
  border-top:2rpx solid rgba(187, 187, 187, 0.31);
  font-size:36rpx;
  text-align: center;
  line-height: 78rpx;
  color:#02BB00;
  float:left;
}


