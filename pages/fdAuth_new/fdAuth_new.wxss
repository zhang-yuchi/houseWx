page{
  height:100%;
}
.container{
  width:100%;
  height:100%;
  background: rgb(248,248,248);
  overflow: hidden
}
.card{
    width:70%;
    height:360rpx;
    background:white;
    margin:80rpx auto;
    margin-bottom:0;
    box-shadow: 0px 0px 10px  gray;
}
.image{
    position:fixed;
    width:70%;
    height:360rpx;
}
.plusLogo{
    position:fixed;
    width:70%;
    height:360rpx;
    margin:0 auto;
}
.uploadId{
    width:90%;
    height:40rpx;
    font-size:24rpx;
    text-align: center;
    line-height:40rpx;
    color:rgb(123,123,123);
    top:240rpx;
    position: fixed;
}
.o{
    top:620rpx;
}
.submit{
    width:80%;
    height:120rpx;
    margin:40rpx auto;
    margin-bottom:0;
    background: rgb(255,217,62);
    color:black;
    text-align: center;
    line-height:120rpx;
    font-size:36rpx;
    letter-spacing: 4rpx;
    font-family:cursive;
    border-radius: 6rpx;
    margin-top:80rpx;

}
.tips{
  text-align: center;
  font-size: 30rpx;
  margin-top: 10rpx;
  color: rgb(169, 169, 169);
}