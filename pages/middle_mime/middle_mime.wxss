  border:20rpx solid white;
  border-radius: 50%;
}
.UbarBox{
  width:90%;
  height:240rpx;
  margin: 0 auto;
  margin-top:40rpx;
  box-shadow: 0 4rpx 10rpx rgb(240,240,240);
}
