<!--pages/myfixed_details/myfixed_details.wxml-->

<scroll-view class="container">
<view class="list-title">
  <view class="list-title-font">待处理</view>
</view>
<view class="list">
  <view class="list-left">选择房源：</view>
  <view class="list-right">{{obj.houseInfo}} {{obj.houseStreet}}</view>
</view>
<view class="list">
  <view class="list-left">联系电话：</view>
  <view class="list-right">{{obj.phone}}</view>
</view>
<view class="list">
  <view class="list-left">维修时间：</view>
  <view class="list-right">{{obj.repaireTime}}</view>
</view>
<view class="list">
  <view class="list-left">维修内容：</view>
  <view class="list-right">{{obj.content}}</view>
</view>
<view class="list">
  <view class="list-left">图片上传：</view>
  <image class="list-right" src="{{obj.houseImgUrl}}" style="height:380rpx;border-radius:10rpx"></image>
</view>
</scroll-view>
